{
  "manifest_version": 2,

  "name": "TrueAutomation.IO",
  "description": "Write your tests faster and make them more reliable with TrueAutomation.IO smart locators.",
  "version": "0.3.2",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "storage",
    "contextMenus",
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "content_scripts": [{
    "matches": ["*://*/*"],
    "js": [
      "content_script.js"
    ]
  }]
}
